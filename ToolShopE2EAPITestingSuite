					"name": "RegisterUser",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"/* Function to generate a random date of birth within valid range (18 to 74 years old) */\r",
									"function getValidDOB() {\r",
									"    let minAge = 18;\r",
									"    let maxAge = 74;\r",
									"    \r",
									"    // Calculate a random age within the range\r",
									"    let randomAge = Math.floor(Math.random() * (maxAge - minAge + 1)) + minAge;\r",
									"\r",
									"    // Get today's date\r",
									"    let today = new Date();\r",
									"\r",
									"    // Calculate birth year by subtracting the age\r",
									"    let birthYear = today.getFullYear() - randomAge;\r",
									"\r",
									"    // Generate a valid DOB in \"YYYY-MM-DD\" format\r",
									"    let birthDate = new Date(birthYear, today.getMonth(), today.getDate());\r",
									"    let dob = birthDate.toISOString().split('T')[0]; // Extract YYYY-MM-DD\r",
									"    \r",
									"    return dob;\r",
									"}\r",
									"\r",
									"/* Set the generated DOB as a Postman variable */\r",
									"const dob = getValidDOB();\r",
									"pm.environment.set(\"dob\", dob);\r",
									"\r",
									"/* Set the password as a Postman env variable */\r",
									"pm.environment.set(\"userPassword\", \"ViratKolhi@12345\");"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
