									"\r",
									"pm.test(\"Status Code & Message is 200 OK\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"    pm.expect(responseData.code).to.be.eql(200);\r",
									"    pm.expect(responseData.status).to.be.eql(\"OK\");\r",
									"});\r",
									"\r",
									"pm.test(\"Response size is equal to or below 4000KB\", function () {\r",
									"    const responseSize = responseData.size(); // { body: 319, header: 342, total: 661 }\r",
									"    pm.expect(responseSize.total).to.be.at.most(4000);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time should be below 1000ms\", () => {\r",
									"    pm.expect(responseData.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify the presence of Server header\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"    pm.expect(responseData.headers.get(\"Server\")).to.contain(\"Apache/\");\r",
									"});\r",
									"\r",
									"pm.test.skip(\"Validate the response body has property message\", function () {\r",
									"    pm.expect(responseData.json().message).to.exist;\r",
									"    pm.expect(responseData.json().message).to.be.eql(\"Payment was successful\");\r",
									"});\r",
									"\r",
									"pm.test(\"Validate the response body to be an array\", function () {\r",
									"    pm.expect(responseData.json()).to.be.an(\"array\");\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/json",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/images",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"images"
							]
						}
					},
					"response": []
				}
			]
		},
		{
			"name": "Invoice_Service",
			"item": [
				{
