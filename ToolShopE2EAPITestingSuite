					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const responseData = pm.response;\r",
									"\r",
									"pm.test(\"Status Code & Message is 200 OK\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"    pm.expect(responseData.code).to.be.eql(200);\r",
									"    pm.expect(responseData.status).to.be.eql(\"OK\");\r",
									"});\r",
									"\r",
									"pm.test(\"Response size is equal to or below 5000KB\", function () {\r",
									"    const responseSize = responseData.size(); // { body: 319, header: 342, total: 661 }\r",
									"    pm.expect(responseSize.total).to.be.at.most(5000);\r",
									"});\r",
									"\r",
									"pm.test(\"Response time should be below 1000ms\", () => {\r",
									"    pm.expect(responseData.responseTime).to.be.below(1000);\r",
									"});\r",
									"\r",
									"pm.test(\"Verify the presence of Server header\", function () {\r",
									"    pm.response.to.have.header(\"Content-Type\");\r",
									"    pm.expect(responseData.headers.get(\"Server\")).to.contain(\"Apache/\");\r",
									"});\r",
									"\r",
									"pm.test.skip('Validate schema for register user response body', function () {\r",
									"    pm.response.to.have.jsonSchema(jsonSchema);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate the property current_page to be present\", function () {\r",
									"    pm.expect(responseData.json().current_page).to.exist;\r",
									"});\r",
									"\r",
									"pm.test(\"Validate response has only 9 products per page\", function () {\r",
									"    const actualProductsCount = responseData.json().data.length;\r",
									"    const expectedProductsCount = responseData.json().per_page;\r",
									"    pm.expect(actualProductsCount).to.be.eql(expectedProductsCount);\r",
									"});\r",
									"\r",
									"pm.test(\"Validate if the price of all products is within the specified range\", function (){\r",
									"    const responseJsonData = responseData.json();\r",
									"\r",
									"    // Fetch the variables set in the Pre-request Script\r",
									"    const lowerPrice = pm.variables.get(\"lowerPrice\");\r",
									"    const upperPrice = pm.variables.get(\"upperPrice\");\r",
									"    \r",
									"    // Ensure the response contains data\r",
									"    pm.expect(responseJsonData.data).to.be.an(\"array\").that.is.not.empty;\r",
									"\r",
									"    // Validate each product's price is within the range\r",
									"    responseJsonData.data.forEach((product) => {\r",
									"        pm.expect(product).to.have.property(\"price\"); // Ensure \"price\" field exists\r",
									"        pm.expect(product.price, `Product ID: ${product.id} | Price ${product.price} is below lower limit`).to.be.at.least(lowerPrice);\r",
									"        pm.expect(product.price, `Product ID: ${product.id} | Price ${product.price} exceeds upper limit`).to.be.at.most(upperPrice);\r",
									"    });\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Function to generate a random price range\r",
									"function getRandomPriceRange() {\r",
									"    let prices = [10, 30, 50]; // Define possible price values\r",
									"\r",
									"    // Randomly pick two different prices\r",
									"    let lowerIndex = Math.floor(Math.random() * prices.length);\r",
									"    let upperIndex;\r",
									"\r",
									"    do {\r",
									"        upperIndex = Math.floor(Math.random() * prices.length);\r",
									"    } while (upperIndex === lowerIndex || prices[upperIndex] < prices[lowerIndex]); // Ensure upper > lower\r",
									"\r",
									"    return {\r",
									"        lower: prices[lowerIndex],\r",
									"        upper: prices[upperIndex]\r",
									"    };\r",
									"}\r",
									"\r",
									"// Generate a random price range\r",
									"let priceRange = getRandomPriceRange();\r",
									"\r",
									"// Store them as Postman variables\r",
									"pm.variables.set(\"lowerPrice\", priceRange.lower);\r",
									"pm.variables.set(\"upperPrice\", priceRange.upper);\r",
									"\r",
									"console.log(priceRange.lower, priceRange.upper);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Accept-Charset",
								"value": "application/json",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{baseUrl}}/products?between={{lowerPrice}}%2C{{upperPrice}}",
							"host": [
								"{{baseUrl}}"
							],
							"path": [
								"products"
							],
							"query": [
								{
									"key": "between",
									"value": "{{lowerPrice}}%2C{{upperPrice}}"
								}
							]
						}
					},
					"response": []
				},
